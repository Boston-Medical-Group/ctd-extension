{
  "manifest_version": 3,
  "name": "Twilio Hubspot-ctd",
  "version": "0.1.4",
  "description": "Integraci√≥n Twilio Flex <> Hubspot",
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage"
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "resources": [
        "jquery.js"
      ]
    },
    {
      "resources": ["hubspot.js", "hubspot.css", "assets/*", "pages/*","jquery.js","env.js","utils.js","languages.js","static/*"],
      "matches": [ "https://app-eu1.hubspot.com/*", "https://flex.twilio.com/*" ]
    }
  ],
  "content_scripts": [
    {
      "matches": [
        "http://app.hubspot.com/*",
        "https://app.hubspot.com/*",
        "https://app-eu1.hubspot.com/*",
        "http://app-eu1.hubspot.com/*",
        "https://app-eu2.hubspot.com/*",
        "http://app-eu2.hubspot.com/*",
        "https://flex.twilio.com/*"
      ],
      "exclude_matches": [
        "*://flex.twilio.com/*",
        "*://localhost/*"
      ],
      "run_at": "document_idle",
      "js": [
        "contentScript.js"
      ],
      "all_frames": true
    },
    {
      "matches": [
        "*://flex.twilio.com/*",
        "*://localhost/*"
      ],
      "run_at": "document_idle",
      "js": [
        "flexScript.js"
      ]
    }
  ]
}